# Archivo de ejemplo para variables de entorno
# Copia este archivo como .env y completa los valores reales

# Google Calendar OAuth (usuario)
CALENDAR_PROVIDER=google
GOOGLE_OAUTH_CLIENT_ID=tu_clave_secreta
GOOGLE_OAUTH_CLIENT_SECRET=tu_clave_secreta
GOOGLE_OAUTH_ACCESS_TOKEN=tu_clave_secreta
GOOGLE_OAUTH_REFRESH_TOKEN=tu_clave_secreta
GOOGLE_CALENDAR_ID=tu_id_calendario
GOOGLE_CALENDAR_TIMEZONE=UTC

# URL base de la API
API_BASE_URL=http://localhost:8000

# LLM APIFREELLM
APIFREELLM_MODEL=apifreellm
APIFREELLM_API_KEY=tu_clave_secreta

# LLM GROQ
GROQ_MODEL=llama-3.1-8b-instant
GROQ_API_KEY=tu_clave_secreta

# LLM a usar: apifreellm o groq
LLM_PROVIDER=groq